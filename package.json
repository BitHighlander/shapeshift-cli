{
  "name": "@pioneer-platform/shapeshift-cli",
  "version": "8.1.42",
  "main": "./lib/index.js",
  "bin": {
    "pioneer": "lib/index.js"
  },
  "_moduleAliases": {
    "@data": "lib/data"
  },
  "scripts": {
    "clean": "rm -Rf node_modules lib",
    "npm": "npm i",
    "npm-update": "npm update",
    "lint": "prettier --write '**/**/*.ts'",
    "dev": "npm run build && node lib/cli.js",
    "start": "npm run build && node lib/cli.js",
    "link": "yarn link @shapeshiftoss/chain-adapters @shapeshiftoss/unchained-client",
    "export-wallet": "npm run build && node lib/index.js export --format citadel --walletId highlander-restore-7",
    "send-btc": "npm run build && npm run dev tx send btc bc1qkumt74m9tl0anhrga37t52xlknu0eag3scg7nr 0.001 --yes",
    "send-eth": "npm run build && npm run dev tx send eth 0xfEb8bf56e554fc47639e5Ed9E1dAe21DfF69d6A9 0.001 --yes",
    "send-osmo": "npm run build && npm run dev tx send osmo osmo1a7xqkxa4wyjfllme9u3yztgsz363dalz3lxtj6 0.001 --yes",
    "run-bridge": "npm run build && node lib/index.ts --auth",
    "create": "npm run build && node lib/index.ts create app",
    "build-test": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "npm i --force -g && pioneer",
    "test-server": "npm i -g --force && pioneer server start",
    "test-generate-wallet": "npm i -g && pioneer export -O -N --password test123 --format citadel --dir ./config",
    "prepublish": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@shapeshiftoss/chain-adapters": "^1.20.1",
    "@shapeshiftoss/hdwallet-native": "^1.18.4",
